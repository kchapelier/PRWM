{
  "name": "three-buffergeometry-to-prwm",
  "version": "1.0.1",
  "description": "Takes a THREE.BufferGeometry and returns an ArrayBuffer containing a PRWM file.",
  "main": "index.js",
  "scripts": {
    "build-dev": "npx browserify ./index.js -d --s threeBuffergeometryToPrwm > ./build/three-buffergeometry-to-prwm.dev.js",
    "build-min": "npx browserify ./index.js --s threeBuffergeometryToPrwm | npx uglifyjs > ./build/three-buffergeometry-to-prwm.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kchapelier/PRWM.git"
  },
  "keywords": [
    "three",
    "threejs",
    "three.js",
    "prwm"
  ],
  "author": "Kevin Chapelier",
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/kchapelier/PRWM/issues"
  },
  "homepage": "https://github.com/kchapelier/PRWM",
  "dependencies": {
    "prwm": "^1.0.0"
  },
  "devDependencies": {
    "uglify-js": "^3.0.0",
    "browserify": "^17.0.0"
  }
}
